curl -X GET \
  '127.0.0.1:5000/profile' \
  -H 'Content-Type: application/json; charset=utf-8' \
  -d '{
  "id": "2"
}'

curl -X PUT \
  '127.0.0.1:5000/profile' \
  -H 'Content-Type: application/json; charset=utf-8' \
  -d '{
  "id": "2",
  "data": {"content": "55"}
}'

curl -X DELETE \
  '127.0.0.1:5000/profile' \
  -H 'Content-Type: application/json; charset=utf-8' \
  -d '{
  "id": "2"
}'

curl -X POST \
  '127.0.0.1:5000/profile' \
  -H 'Content-Type: application/json; charset=utf-8' \
  -d '{
  "id": "2",
  "content": "123456",
  "location": "1234",
  "username": "1234568"
}'

curl -X GET \
  '127.0.0.1:5000/wx/login?jscode=1' \
  -H 'Content-Type: application/json; charset=utf-8' 


curl -X PUT \
  '127.0.0.1:5000/profile/history' \
  -H 'Content-Type: application/json; charset=utf-8' \
  -d '{
  "id": "2",
  "data": {"location":"1234", "time":"123"}
}'

curl -X GET \
  '127.0.0.1:5000/profiles?id=1' \
  -H 'Content-Type: application/json; charset=utf-8' 


curl -X PUT \
  '127.0.0.1:5000/profile/history' \
  -H 'Content-Type: application/json; charset=utf-8' \
  -d '{
  "id": "ol3Wl4mvorVlzjBDhaDsfpMsCAiU",
  "history_id": "p0mrkj", 
  "data": {"addr":"1234", "time":"123"}
}'